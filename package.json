{
  "name": "lendsqr_be_test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=testing vitest run",
    "test:coverage": "NODE_ENV=testing vitest run --coverage",
    "build": "tsc",
    "run:migrate": "tsnd src/db/migrate.ts",
    "dev:migrate": "knex migrate:latest --env development",
    "test:migrate": "knex migrate:latest --env testing",
    "test:migrate:rollback": "knex migrate:rollback --all --env testing",
    "dev:migrate:up": "knex migrate:up --env development",
    "prod:migrate": "knex migrate:latest --env production",
    "prod:migrate:up": "pnpx knex migrate:up --env production",
    "start": "node dist/src/server.js",
    "dev": "tsnd --respawn --transpile-only src/server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@hono/node-server": "^1.17.1",
    "@hono/zod-validator": "^0.7.2",
    "argon2": "^0.43.1",
    "dotenv": "^17.2.1",
    "hono": "^4.8.9",
    "knex": "^3.1.0",
    "mysql2": "^3.14.2",
    "zod": "^4.0.10",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
